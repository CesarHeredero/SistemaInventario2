
@{
    Layout = "~/Views/Shared/_Layout.cshtml";
}

<br />

<div class="row">
    <div class="col-6">
        <h2 class="text-primary">Historial de Inventarios</h2>
    </div>
    <div class="col-6 text-right">
        <a class="btn btn-primary" asp-action="NuevoInventario"><i class="fas fa-plus"></i> Crear nuevo Inventario</a>
    </div>

</div>
<br />

<div class="mt-4 rounded">
    <table id="tblDatos" class="table table-striped" style="width:100%">
        <thead class="thead-dark">
            <tr class="table-info">
                <th>Fecha Inicio</th>
                <th>Fecha fin</th>
                <th>Bodega</th>
                <th>Usuario</th>
                <th></th>
            </tr>
        </thead>
    </table>
</div>
<div class="col-6 p-2">
    <a class="btn btn-success" asp-action="Index">
        Regresar
    </a>
</div>

@section Scripts{
    <script src="~/js/historial.js"></script>
}